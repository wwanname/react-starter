{
  "extends": ["eslint:recommended", "next/core-web-vitals", "next/typescript"],
  "plugins": ["react, prettier"],
  "rules": {
    "react-compiler/react-compiler": "error",
    "react/react-in-jsx-scope": "off"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
